PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX aasrange: <https://admin-shell.io/aas/3/0/RC01/Range/>

DELETE {
    ?prop aasrange:min ?min ; 
        aasrange:max ?max ; 
        aasrange:valueType ?valtype .
}
INSERT {
    ?prop aasrange:min ?propermin . 
    ?prop aasrange:max ?propermax . 
}
WHERE {
    ?prop aasrange:min ?min ; 
        aasrange:max ?max ; 
        aasrange:valueType ?valtype .
    FILTER (?valtype = "float")
    BIND(STRDT(STR(?min), xsd:float) AS ?propermin)
    BIND(STRDT(STR(?max), xsd:float) AS ?propermax)
}
